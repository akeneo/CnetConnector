{
    "name": "akeneo/cnet-connector",
    "type": "symfony-bundle",
    "description": "Akeneo PIM CNET connector",
    "keywords": ["pim", "akeneo", "cnet", "integration", "connector"],
    "homepage": "http://www.akeneo.com",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git",
            "branch": "master"
        }
    ],
    "require": {
        "akeneo/pim-community-dev": "~1.6.0",
        "akeneo-labs/custom-entity-bundle": "1.8.*",
        "akeneo/extended-measure-bundle": "dev-master@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "3.0.0",
        "squizlabs/php_codesniffer": "2.3.*",
        "phpmd/phpmd": "2.3.*",
        "friendsofphp/php-cs-fixer": "@stable",
        "henrikbjorn/phpspec-code-coverage": "3.0.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Pim\\Bundle\\CnetConnectorBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
